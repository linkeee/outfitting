                                                                             prefWidth="64.0" text="公式ID"/>
                                                                <TableColumn fx:id="tcTableFormulaCol2" editable="false"
                                                                             prefWidth="122.0" text="待求变量名"/>
                                                                <TableColumn fx:id="tcTableFormulaCol3" editable="false"
                                                                             prefWidth="121.0" text="待求变量ID"/>
                                                                <TableColumn fx:id="tcTableFormulaCol4" editable="false"
                                                                             minWidth="0.0" prefWidth="201.0"
                                                                             text="计算代数式"/>
                                                                <TableColumn fx:id="tcTableFormulaCol5" editable="false"
                                                                             minWidth="1.0" prefWidth="88.0"
                                                                             text="判断规则"/>
                                                                <TableColumn fx:id="tcTableFormulaCol6" editable="false"
                                                                             prefWidth="205.0" text="规则表达式"/>
                                                                <TableColumn fx:id="tcFormulaLastColumn"
                                                                             editable="false" prefWidth="131.0"
                                                                             text="公式说明"/>
                                                            </columns>
                                                            <columnResizePolicy>
                                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                            </columnResizePolicy>
                                                        </TableView>
                                                    </children>
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </VBox>
                                                <VBox alignment="CENTER" spacing="15.0">
                                                    <children>
                                                        <Button fx:id="btEditFormula" contentDisplay="CENTER"
                                                                minWidth="-Infinity" mnemonicParsing="false"
                                                                prefHeight="23.0" prefWidth="90.0" text="编辑公式"/>
                                                        <Button fx:id="btDeleteFormula" contentDisplay="CENTER"
                                                                minWidth="-Infinity" mnemonicParsing="false"
                                                                prefHeight="23.0" prefWidth="86.0" text="删除公式"/>
                                                        <Button fx:id="btCheckAlteration" defaultButton="true"
                                                                minWidth="-Infinity" mnemonicParsing="false"
                                                                prefHeight="60.0" prefWidth="88.0" text="验证公式库有效性"
                                                                wrapText="true"/>
                                                    </children>
                                                </VBox>
                                            </children>
                                            <StackPane.margin>
                                                <Insets left="20.0"/>
                                            </StackPane.margin>
                                        </HBox>
                                    </children>
                                </StackPane>
                            </children>
                        </AnchorPane>
                    </content>
                </TitledPane>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
