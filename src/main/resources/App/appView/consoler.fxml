                                                                          mnemonicParsing="false"
                                                                          GridPane.columnIndex="1"
                                                                          GridPane.rowIndex="2"/>
                                                            </children>
                                                            <padding>
                                                                <Insets bottom="10.0" left="10.0" right="10.0"
                                                                        top="10.0"/>
                                                            </padding>
                                                        </GridPane>
                                                        <Button fx:id="btQueryVariable" minWidth="-Infinity"
                                                                mnemonicParsing="false" prefHeight="35.0"
                                                                prefWidth="80.0" text="查询">
                                                            <HBox.margin>
                                                                <Insets/>
                                                            </HBox.margin>
                                                        </Button>
                                                    </children>
                                                </HBox>
                                                <StackPane pickOnBounds="false" VBox.vgrow="ALWAYS">
                                                    <children>
                                                        <TableView fx:id="tvTableVariable"
                                                                   nodeOrientation="LEFT_TO_RIGHT" prefHeight="160.0"
                                                                   tableMenuButtonVisible="true">
                                                            <columns>
                                                                <TableColumn fx:id="tcTableVariableCol1"
                                                                             editable="false" minWidth="0.0"
                                                                             text="变量ID"/>
                                                                <TableColumn fx:id="tcTableVariableCol2"
                                                                             editable="false" minWidth="0.0"
                                                                             text="变量名"/>
                                                                <TableColumn fx:id="tcTableVariableCol3"
                                                                             editable="false" minWidth="0.0"
                                                                             text="变量类型"/>
                                                                <TableColumn fx:id="tcTableVariableCol4" minWidth="0.0"
                                                                             text="变量描述"/>
                                                                <TableColumn fx:id="tcTableVariableCol5" minWidth="0.0"
                                                                             text="设备描述"/>
                                                                <TableColumn fx:id="tcTableVariableCol6" minWidth="0.0"
                                                                             text="变量范围"/>
                                                            </columns>
                                                            <StackPane.margin>
                                                                <Insets/>
                                                            </StackPane.margin>
                                                            <columnResizePolicy>
                                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                            </columnResizePolicy>
                                                        </TableView>
                                                    </children>
                                                </StackPane>
                                                <HBox alignment="CENTER" pickOnBounds="false" spacing="50.0"
                                                      VBox.vgrow="ALWAYS">
                                                    <children>
                                                        <Button fx:id="btNewVariable" minWidth="-Infinity"
                                                                mnemonicParsing="false" prefHeight="33.0"
                                                                prefWidth="87.0" text="新增变量"/>
                                                        <Button fx:id="btEditVariable" layoutX="96.0" layoutY="30.0"
                                                                minWidth="-Infinity" mnemonicParsing="false"
                                                                prefHeight="33.0" prefWidth="85.0" text="修改变量"/>
                                                        <Button fx:id="btDeleteVariable" minWidth="-Infinity"
                                                                mnemonicParsing="false" prefHeight="31.0"
                                                                prefWidth="83.0" text="删除变量"/>
                                                    </children>
                                                    <padding>
                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                    </padding>
                                                </HBox>
                                            </children>
                                        </VBox>
                                    </children>
                                </StackPane>
                            </content>
                            <padding>
                                <Insets left="10.0"/>
                            </padding>
                        </TitledPane>
                    </children>
                </HBox>
                <TitledPane animated="false" collapsible="false" text="公式查询" VBox.vgrow="ALWAYS">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0">
                            <children>
                                <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <children>
                                        <HBox alignment="CENTER_LEFT" spacing="20.0">
                                            <children>
                                                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                                    <children>
                                                        <HBox alignment="CENTER_LEFT" spacing="50.0"
                                                              VBox.vgrow="ALWAYS">
                                                            <children>
                                                                <HBox alignment="CENTER_LEFT" spacing="10.0"
                                                                      HBox.hgrow="ALWAYS">
                                                                    <children>
                                                                        <Label minWidth="-Infinity" prefHeight="15.0"
                                                                               prefWidth="79.0" text="查询条件类型"/>
