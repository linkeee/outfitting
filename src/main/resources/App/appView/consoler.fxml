                                                                <ColumnConstraints hgrow="ALWAYS" maxWidth="115.0"
                                                                                   minWidth="-Infinity"
                                                                                   prefWidth="86.0"/>
                                                                <ColumnConstraints hgrow="ALWAYS" maxWidth="174.0"
                                                                                   minWidth="10.0" prefWidth="174.0"/>
                                                            </columnConstraints>
                                                            <rowConstraints>
                                                                <RowConstraints minHeight="-Infinity" prefHeight="30.0"
                                                                                vgrow="SOMETIMES"/>
                                                                <RowConstraints minHeight="-Infinity" prefHeight="30.0"
                                                                                vgrow="SOMETIMES"/>
                                                                <RowConstraints minHeight="-Infinity" prefHeight="30.0"
                                                                                vgrow="SOMETIMES"/>
                                                            </rowConstraints>
                                                            <children>
                                                                <Label prefHeight="15.0" prefWidth="39.0" text="变量名"/>
                                                                <TextField fx:id="tfStringQueryVariable"
                                                                           prefHeight="30.0" prefWidth="104.0"
                                                                           promptText="精确查询" GridPane.columnIndex="1"
                                                                           GridPane.hgrow="ALWAYS"
                                                                           GridPane.vgrow="ALWAYS"/>
