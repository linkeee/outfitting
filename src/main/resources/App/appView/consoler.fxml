                                                                        <Insets/>
                                                                    </HBox.margin>
                                                                </TextField>
                                                                <Button fx:id="btQueryFormula" minWidth="-Infinity"
                                                                        mnemonicParsing="false" prefHeight="23.0"
                                                                        prefWidth="88.0" text="立即查询"/>
