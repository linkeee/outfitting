<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="App.formulalib.Consoler">
    <children>
        <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <HBox spacing="10.0" VBox.vgrow="ALWAYS">
                    <children>
                        <TitledPane collapsible="false" text="公式编辑" HBox.hgrow="ALWAYS">
                            <content>
                                <StackPane>
                                    <children>
                                        <VBox spacing="30.0">
                                            <children>
                                                <HBox alignment="CENTER" spacing="20.0">
                                                    <children>
                                                        <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                                            <children>
                                                                <Label minWidth="-Infinity" prefHeight="15.0"
                                                                       prefWidth="52.0" text="待求变量"/>
                                                                <TextField fx:id="tfStringNewVariable" prefHeight="30.0"
                                                                           prefWidth="100.0" promptText="请输入待求变量名"
                                                                           VBox.vgrow="ALWAYS"/>
                                                            </children>
                                                        </VBox>
                                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="="
                                                              HBox.hgrow="ALWAYS">
                                                            <font>
                                                                <Font size="46.0"/>
                                                            </font>
                                                            <HBox.margin>
                                                                <Insets/>
                                                            </HBox.margin>
                                                        </Text>
                                                        <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                                            <children>
                                                                <Label minWidth="-Infinity" prefHeight="15.0"
                                                                       prefWidth="69.0" text="计算代数式"/>
                                                                <TextField fx:id="tfStringNewAlgebraic"
                                                                           prefHeight="30.0" prefWidth="300.0"
                                                                           promptText="请输入计算代数式" VBox.vgrow="ALWAYS"/>
                                                            </children>
                                                        </VBox>
                                                        <VBox layoutX="10.0" layoutY="10.0" spacing="10.0"
                                                              HBox.hgrow="ALWAYS">
                                                            <children>
                                                                <Label minWidth="-Infinity" prefHeight="15.0"
                                                                       prefWidth="55.0" text="公式备注"/>
                                                                <TextField fx:id="tfStringNewFormulaRemark"
                                                                           prefHeight="30.0" prefWidth="161.0"
                                                                           promptText="请输入备注" VBox.vgrow="ALWAYS"/>
                                                            </children>
                                                        </VBox>
                                                    </children>
                                                </HBox>
                                                <VBox spacing="20.0"
                                                      style="-fx-border-style: solid inside; -fx-border-color: black; -fx-border-insets: 0; -fx-border-radius: 3; -fx-border-width: 2; -fx-padding: 15; -fx-background-color: e4e4e4;"
                                                      VBox.vgrow="ALWAYS">
                                                    <children>
                                                        <FlowPane>
                                                            <children>
                                                                <Label minWidth="-Infinity" prefHeight="15.0"
                                                                       prefWidth="90.0" text="是否使用规则："/>
                                                                <CheckBox fx:id="cbNewIsRuled" mnemonicParsing="false"/>
                                                            </children>
                                                        </FlowPane>
                                                        <HBox spacing="40.0">
                                                            <children>
                                                                <VBox spacing="10.0">
                                                                    <children>
                                                                        <Label minWidth="-Infinity" prefHeight="15.0"
                                                                               prefWidth="57.0" text="规则变量"/>
                                                                        <TextField fx:id="tfStringNewRestrictedVariable"
                                                                                   prefWidth="100.0"
                                                                                   VBox.vgrow="ALWAYS"/>
                                                                    </children>
                                                                </VBox>
                                                                <HBox alignment="BOTTOM_LEFT" spacing="10.0">
                                                                    <children>
                                                                        <Label minWidth="-Infinity" prefHeight="15.0"
                                                                               prefWidth="43.0" text="下限值"/>
                                                                        <TextField fx:id="tfDoubleNewLowerBound"
                                                                                   HBox.hgrow="ALWAYS">
                                                                            <HBox.margin>
                                                                                <Insets/>
                                                                            </HBox.margin>
                                                                        </TextField>
                                                                    </children>
                                                                </HBox>
                                                                <HBox alignment="BOTTOM_LEFT" spacing="10.0">
                                                                    <children>
                                                                        <Label minWidth="-Infinity" prefHeight="15.0"
                                                                               prefWidth="44.0" text="上限值"/>
                                                                        <TextField fx:id="tfDoubleNewUpperBound"
                                                                                   HBox.hgrow="ALWAYS"/>
                                                                    </children>
                                                                </HBox>
                                                            </children>
                                                        </HBox>
                                                        <VBox spacing="10.0" VBox.vgrow="ALWAYS">
                                                            <children>
                                                                <Label minWidth="-Infinity" prefHeight="15.0"
                                                                       prefWidth="53.0" text="规则备注"/>
                                                                <TextField fx:id="tfStringNewRuleDescription"
                                                                           VBox.vgrow="ALWAYS"/>
                                                            </children>
                                                        </VBox>
                                                    </children>
                                                </VBox>
                                                <StackPane VBox.vgrow="ALWAYS">
                                                    <children>
                                                        <Button fx:id="submitFormula" minWidth="-Infinity"
                                                                mnemonicParsing="false" prefHeight="23.0"
                                                                prefWidth="102.0" text="添加公式"/>
                                                    </children>
                                                    <VBox.margin>
                                                        <Insets/>
                                                    </VBox.margin>
                                                </StackPane>
                                            </children>
                                        </VBox>
                                    </children>
                                    <padding>
                                        <Insets bottom="10.0" left="30.0" right="30.0" top="10.0"/>
                                    </padding>
                                </StackPane>
                            </content>
                        </TitledPane>
                        <TitledPane animated="false" collapsible="false" pickOnBounds="false" text="变量查询"
                                    HBox.hgrow="ALWAYS">
                            <content>
                                <StackPane pickOnBounds="false">
                                    <children>
                                        <VBox pickOnBounds="false" StackPane.alignment="CENTER">
                                            <children>
                                                <HBox alignment="CENTER_LEFT" pickOnBounds="false" spacing="10.0"
                                                      VBox.vgrow="ALWAYS">
                                                    <children>
                                                        <GridPane alignment="CENTER_LEFT" maxWidth="-Infinity"
                                                                  minWidth="-Infinity" pickOnBounds="false" vgap="8.0"
                                                                  HBox.hgrow="ALWAYS">
