                                                                <TextField fx:id="tfStringQueryDescription"
                                                                           prefWidth="200.0" promptText="变量描述的模糊查询"
                                                                           GridPane.columnIndex="1"
                                                                           GridPane.hgrow="ALWAYS" GridPane.rowIndex="1"
                                                                           GridPane.vgrow="ALWAYS"/>
